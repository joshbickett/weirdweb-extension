{
  "manifest_version": 3,
  "name": "Weirdweb.ai - An AI powered web game.",
  "version": "0.0.0.1",
  "description": "Weirdweb is a Chrome Extension based game that changes your page with AI. Only one link on the page will take you to the winning page!",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "robot.png"
  },
  "icons": {
    "128": "robot.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["contentScript.bundle.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "robot.png",
        "*.png",
        "*.svg",
        "*.gif",
        "*.woff",
        "*.tff"
      ],
      "matches": ["https://*/*", "http://*/*"]
    }
  ]
}
